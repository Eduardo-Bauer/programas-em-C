#include<stdio.h>
void main()
{
         float p, u, r;
         printf("digite os valores de p e u:\n");
         scanf("%f%f", &p, &u);
         printf("digite a razao da progressao:\n");
         scanf("%f", &r);
         p = (p+u)/r;
         printf("%f", p);
}
