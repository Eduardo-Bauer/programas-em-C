#include<stdio.h>
#include<math.h>
void main()
{
         int i, n, quadrado, cubo;
         float raiz;
         i=0;
         do {
                  printf("digite um numero\n");
                  scanf("%d", &n);
                  printf("----------------------------------\n");
                  raiz = sqrt(n);
                  quadrado = n*n;
                  cubo = n*n*n;
                  printf("%d     ", n);
                  printf("%d     ", quadrado);
                  printf("%d     ", cubo);
                  printf("%f     \n", raiz);
                  i++;
         }while(i<20);
}
