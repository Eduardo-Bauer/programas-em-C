#include<stdio.h>
void main()
{
         int n, i, maior;
         maior = 0;
         for(i=0;i<50;i++){
                  printf("digite um numero\n");
                  scanf("%d", &n);
                  if(n>maior){
                           maior = n;
                  }
         }
         printf("o maior valor é: %d\n", maior);
}
