#include<stdio.h>
void main()
{
         int x[100], i;
         for(i=0;i<100;i++){
                  printf("digite os valores de x\n");
                  scanf("%d", &x[i]);
         }
         for(i=0;i<100;i++){
                  printf("%d ", x[i]);
                  if(x[i]<0){
                           x[i] = 1;
                  }
         }
         printf("\n");
         for(i=0;i<100;i++){
                  printf("%d ", x[i]);
         }
}
