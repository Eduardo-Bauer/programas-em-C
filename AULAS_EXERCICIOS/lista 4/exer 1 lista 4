#include<stdio.h>
void main()
{
         int v[6], i, negativos;
         negativos = 0;
         for(i=0;i<6;i++){
                  printf("digite os valores de v\n");
                  scanf("%d", &v[i]);
                  if(v[i]<0){
                           negativos++;
                  }
         }
         for(i=0;i<6;i++){
                  printf("%d ", v[i]);
         }
         printf("\n");
         printf("o total de negativos e: %d", negativos);
}
