#include<stdio.h>
void main()
{
         int v[10], negativos, i;
         negativos = 0;
         for(i=0;i<10;i++){
                  printf("digite os valores de v\n");
                  scanf("%d", &v[i]);
         }
         for(i=0;i<10;i++){
                  printf("%d ", v[i]);
         }
         printf("\n");
         for(i=0;i<10;i++){
                  if(v[i]<0){
                           negativos++;
                           v[i]=1;
                           printf("posicao trocada %d\n", i); 
                  }
         }
         printf("negativos %d\n", negativos);
         for(i=0;i<10;i++){
                  printf("%d ", v[i]);
         }
}

