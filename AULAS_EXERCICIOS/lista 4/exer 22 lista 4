#include<stdio.h>
void main()
{
         int v[20], i, j, menor, aux;
         for(i=0;i<20;i++){
                  printf("digite os valores:\n");
                  scanf("%d", &v[i]);
         }
         for(i=0;i<20;i++){
                  printf("%d ", v[i]);
         }
         printf("\n");
         for(i=0;i<20;i++){
                  menor = v[i];
                  for(j=i+1;j<20;j++){
                           if(v[j]<menor){
                                          menor = v[j];
                                          aux = v[i];
                                    v[i] = v[j];
                                    v[j] = aux;
                           }
                  }
         }
         for(i=0;i<20;i++){
                  printf("%d ", v[i]);
         }
}
