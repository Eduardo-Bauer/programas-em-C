#include<stdio.h>
void main()
{
         int a[10], i;
         for(i=0;i<10;i++){
                  printf("digite os valores de a\n");
                  scanf("%d", &a[i]);
         }
         for(i=0;i<10;i++){
                  if(a[i]==5){
                           printf("%d ", a[i]);
                  }
         }
}
