#include<stdio.h>
void main()
{
         int c[50], maior, i;
         maior = -999999;
         for(i=0;i<50;i++){
                  printf("digite os valores:\n");
                  scanf("%d", &c[i]);
         }
         for(i=0;i<50;i++){
                  printf("%d ", c[i]);
         }
         printf("\n");
         
         for(i=0;i<50;i++){
                  if(c[i]>maior){
                           maior = c[i];
                  }
         }
         printf("o maior %d", maior);
}
