#include<stdio.h>
void main()
{
         int a[10], b[10], i, j, c[10], igual, cont;
         cont = 0;
         for(i=0;i<10;i++){
                  printf("digite os valores de a:\n");
                  scanf("%d", &a[i]);
         }
         for(i=0;i<10;i++){
                  printf("digite os valores de b:\n");
                  scanf("%d", &b[i]);
         }
         for(i=0;i<10;i++){
                  printf("%d ", a[i]);
         }
         printf("\n");
         for(i=0;i<10;i++){
                  printf("%d ", b[i]);
         }
         printf("\n");
         for(i=0;i<10;i++){
                  igual = 0;
                  for(j=0;j<10;j++){
                           if(a[i] == b[j]){
                                    igual = 1;
                                    j = 10;
                           }
                  }
                  if(igual == 1){
                           c[cont] = a[i];
                           cont++;
                  }
         }
         for(i=0;i<cont;i++){
                  printf("%d ", c[i]);
         }
}
