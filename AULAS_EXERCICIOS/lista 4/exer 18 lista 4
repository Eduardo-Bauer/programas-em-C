#include<stdio.h>
void main()
{
         int v[20], i, j, cont;
         cont = 20;
         for(i=0;i<20;i++){
                  printf("digite os valores de v:\n");
                  scanf("%d", &v[i]);
         }
         for(i=0;i<20;i++){
                  printf("%d ", v[i]);
         }
         printf("\n");
         for(i=0;i<cont;){
                  if(v[i]<=0){
                           for(j=i;j<cont-1;j++){
                                    v[j] = v[j+1];
                           }
                           cont = cont-1;
                  } else {
                           i++;
                  }
         }
         for(i=0;i<cont;i++){
                  printf("%d ", v[i]);
         }
}
