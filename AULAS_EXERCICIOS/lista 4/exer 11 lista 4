#include<stdio.h>
void main()
{
         int v[10], escalar, i;
         for(i=0;i<10;i++){
                  printf("digite os valores de v:\n");
                  scanf("%d", &v[i]);
         }
         
         printf("digite o valores escalar:\n");
         scanf("%d", &escalar);

         for(i=0;i<10;i++){
                  v[i] = v[i]*escalar;
         }
         for(i=0;i<10;i++){
                  printf("%d ", v[i]);
         }
}
